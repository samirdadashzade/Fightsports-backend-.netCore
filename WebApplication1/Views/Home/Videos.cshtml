        $('.search_button').click(function(){
            var data = $('.search_area').val()
            $.ajax({
                url:"/Home/Search/" + data,
                type:"GET",
                dataType: "json",
                success:function(data){
                   $('#modal03').modal('show');
                    var searchList = $('.search_list')
                    searchList.empty()

                    $.each(data,function(index,val){
                        var result = '<div class="search_list_item">' + 
                                        '<a class="list_item_href" href="/Home/News/' + val.newsId + '">' 
                                            + '<div>' + val.newsTitle + '</div>' 
                                            + '<div class="news_search_bg">' + val.newsBigTitle + '</div>' + 
                                         '</a>' + 
                                      '</div>';
                        searchList.append(result)
                    })
                }
            })
        })
